<http://d.opencalais.com/dochash-1/4178f973-185c-3be5-a4c3-a9f7ca184046>
        a                  <http://s.opencalais.com/1/type/sys/DocInfo> ;
        c:calaisRequestID  "bfa82fb7-9fe4-e579-14d9-14264236fa60" ;
